{
  "name": "@unggiapro121/ari-design",
  "version": "1.10.0",
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/unggiapro121/ari-design"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/unggiapro121"
  },
  "scripts": {
    "start": "storybook dev",
    "dev": "npm-run-all --parallel storybook",
    "build": "storybook build",
    "buildAndServe": "npm-run-all build serve",
    "serve": "http-server storybook-static",
    "storybook": "storybook dev -p 6006",
    "prepublishOnly": "npm run bundle",
    "publish:beta": "npm publish --tag beta",
    "bundle": "npm-run-all barrel vite",
    "bundle:analyze": "ANALYZE=true npm run bundle",
    "barrel": "barrelsby -D -q -d ./src -e \"\\.(model|stories|test|doc|Typography)\\.\" -e _ -E",
    "eslint": "eslint . --ext tsx,ts",
    "release": "changeset version && node ./.changeset/copyChangelog.js",
    "release:commit": "git commit -am \"Release: ${npm_package_version}\"",
    "release:tag": "changeset tag",
    "eslint-fix": "eslint . --ext tsx,ts --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint-fix": "stylelint --fix \"src/**/!(_icons|_colors)*.scss\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:coverage:serve": "http-server coverage",
    "test:snapshot": "vitest --update",
    "generate:component": "plop",
    "auth": "better-vsts-npm-auth",
    "link": "concurrently npm:barrel npm:vite:watch",
    "check-code-quality": "concurrently --success all 'npm:eslint' 'npm:stylelint'",
    "pre-push": "concurrently --kill-others-on-fail --success all 'npm:check-code-quality' 'npm:test' 'npm:build' 'npm:bundle'",
    "typescript": "tsc",
    "typescript:watch": "tsc --watch",
    "vite:watch": "vite build --watch --mode library",
    "vite": "vite build --mode library -l info"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
