    "@storybook/addon-styling-webpack": "^1.0.0",
    "@storybook/blocks": "^8.0.1",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.0.1",
    "@storybook/react-vite": "^8.0.1",
    "@tanstack/react-table": "^8.7.9",
    "@testing-library/jest-dom": "^5.16.5",
